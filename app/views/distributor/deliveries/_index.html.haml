.row
  %table.span12
    %thead
      %tr
        %th Order #
        %th Delivery #
        %th Status
        %th Date
        %th Route

    %tbody
      - @deliveries.each do |delivery|
        %tr
          %td= delivery.order.id
          %td= delivery.id
          %td= delivery.status
          %td= delivery.date
          %td= delivery.route.name
